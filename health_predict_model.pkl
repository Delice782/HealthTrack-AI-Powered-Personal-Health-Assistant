import numpy as np
from sklearn.linear_model import LogisticRegression
import pickle

# Sample dataset with features: [activity_level, sleep_duration, diet_quality, age]
X = np.array([
    [5, 7, 8, 25],
    [3, 6, 5, 50],
    [8, 8, 9, 35],
    [2, 5, 4, 60],
    [7, 7, 7, 45],
])

# Health status: 0 - Unhealthy, 1 - Healthy
y = np.array([1, 0, 1, 0, 1])

# Train a logistic regression model
model = LogisticRegression()
model.fit(X, y)

# Save the trained model
with open('health_predict_model.pkl', 'wb') as f:
    pickle.dump(model, f)
